{
  "name": "pdf-editor",
  "version": "1.0.0",
  "description": "Web-based PDF editor",
  "type": "module",
  "main": "src/server/index.ts",
  "scripts": {
    "prestart": "node cleanup-ports.js",
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "server": "node --loader ts-node/esm src/server/index.ts",
    "client": "webpack serve --mode development --open --config webpack.config.cjs",
    "build": "webpack --mode production --config webpack.config.cjs && tsc -p tsconfig.json"
  },
  "dependencies": {
    "@tinymce/tinymce-react": "^4.3.0",
    "@types/html-pdf-node": "^1.0.2",
    "@types/uuid": "^10.0.0",
    "canvas": "^3.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "html-pdf-node": "^1.0.8",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.1",
    "mammoth": "^1.9.0",
    "multer": "^1.4.5-lts.1",
    "officegen": "^0.6.5",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^1.1.1",
    "pdf.js-extract": "^0.2.1",
    "pdf2html": "^3.1.0",
    "pdfjs-dist": "^3.11.174",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-pdf": "^9.2.1",
    "sharp": "^0.33.5",
    "tesseract.js": "^6.0.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.2.5",
    "@types/react": "^18.2.7",
    "@types/react-dom": "^18.2.4",
    "@types/react-pdf": "^6.2.0",
    "concurrently": "^8.0.1",
    "css-loader": "^6.8.1",
    "html-webpack-plugin": "^5.5.1",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "webpack": "^5.84.1",
    "webpack-cli": "^5.1.1",
    "webpack-dev-server": "^4.15.0"
  }
}
